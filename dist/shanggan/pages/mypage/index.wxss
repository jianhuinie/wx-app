page {
  height: 100%;
  background-color: #f8f8f8;
}
page .my-page {
  border-top: 2rpx solid #dcddde;
}
page .my-page .info-item {
  background: #fff;
  position: relative;
  font-size: 30rpx;
  color: #2D2D2E;
  line-height: 30rpx;
  height: 90rpx;
  box-sizing: border-box;
  padding-top: 30rpx;
  border-bottom: 2rpx solid #f8f8f8;
}
page .my-page .info-item .icon-right {
  width: 20px;
  height: 20px;
  position: absolute;
  right: 3%;
  top: 26rpx;
}
page .my-page .info-item .status-text {
  font-size: 26rpx;
  color: #9D9D9E;
  line-height: 26rpx;
  position: absolute;
  right: 10%;
  top: 34rpx;
}
page .my-page .info-item:last-child {
  border: none;
}
page .my-page button.info-item {
  text-align: left;
  padding-left: 0;
}
page .my-page button.info-item:after {
  display: none;
}
page .my-page .user-info {
  height: 130rpx;
  margin: 20rpx 0;
  padding-top: 22rpx;
  padding-left: 30rpx;
  border: none;
}
page .my-page .user-info .icon-right {
  top: 50rpx;
}
page .my-page .user-info .user-avatar {
  display: inline-block;
}
page .my-page .user-info .user-avatar image {
  width: 45px;
  height: 45px;
  border-radius: 50%;
}
page .my-page .user-info text {
  font-size: 34rpx;
  color: #3D3D3E;
  line-height: 34rpx;
  position: relative;
  left: 20rpx;
  bottom: 30rpx;
}
page .my-page .outer-box {
  padding-left: 30rpx;
  background: #fff;
}
page .my-page .about-app {
  margin-top: 20rpx;
}
page .my-page .about-app contact-button {
  width: 100%;
}
page .my-page .about-app contact-button text {
  float: left;
  margin-right: 73%;
}
